datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model user{
  id        Int      @id @default(autoincrement())
  firstName String 
  lastName String
  password String
  phoneNumber String @unique
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}